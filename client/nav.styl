body
	background-color #eee
	
.navbar
	width 100%
	height 30px !important
	background-color #161718
	border none
	cursor pointer
	border-radius 0px
.navbar-header
	width 100%
	span.brand
		line-height 50px
		color #555
		text-transform uppercase
		margin-left 10px
		font-family 'Open Sans', sans-serif
		button
			margin-left 10px

	span#menu
		margin-right 5px
		color #666
		transition-property -webkit-transform
		transition-duration .5s
		font-size 1em
#menu-btn
	background-color transparent


.btn:focus,.btn:active 
	outline none !important
	
.dropdown-menu 
	max-width 40px !important
	min-width 60px !important
	min-height 150px !important
	box-shadow 0px 0px 0px !important
#dd
	max-width 20px !important
	margin-left 0px
	margin-top 0px
	border-radius 0
	background-color transparent
	border-right 0px solid #0026BF
	border-bottom 0px
	border-top 0px
	border-left 0px
	
	li
		margin 0 auto
		padding 10px 5px
		min-height 0px !important
		//border 1px solid red
		p
			//border 1px solid yellow
			line-height 10px
			margin-left 5px
		
	
		
#top
	margin-top 5px
	margin-right 10px
	animation-duration .3s
	color #eee
	transition-property color
	transition-duration .3s
	
	span
		color #333
		margin-left 10px
#top:hover
	color #333
	

#rand
	margin-top 5px
	margin-right 10px
	transition-property color
	transition-duration .3s
	animation-duration .3s
	color #eee
	animation-duration .3s
	animation-delay .05s
	span
		color #333
		margin-left 10px
#rand:hover
	color #333
#rand
	margin 0px
	animation-duration .3s
	animation-delay .05s
			
